      <TextField fx:id="rue" layoutX="127.0" layoutY="146.0" onAction="#rue" promptText="Rue" />
      <Button fx:id="ajouter" layoutX="345.0" layoutY="338.0" mnemonicParsing="false" onAction="#ajouter" text="Ajouter" />
      <Text layoutX="148.0" layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajout D'un Fournisseur" />
      <Button fx:id="Retour" layoutX="23.0" layoutY="338.0" mnemonicParsing="false" onAction="#retour" text="Retour" />
